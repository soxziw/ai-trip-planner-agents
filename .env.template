# Copy this file to .env and fill in your API keys

# OpenAI API key for the language model
OPENAI_API_KEY=your_openai_api_key_here

# RapidAPI key for Airbnb and Booking.com APIs
RAPIDAPI_KEY=your_rapidapi_key_here

# Yelp API key for restaurant search
YELP_API_KEY=your_yelp_api_key_here

# TripAdvisor API key for attractions
TRIPADVISOR_API_KEY=your_tripadvisor_api_key_here

# Google Maps API key for directions
GOOGLE_API_KEY=your_google_api_key_here